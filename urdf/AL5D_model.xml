<robot name="AL5D">
    <link name="base_link" />
    <link name="shoulder" />
     <link name="upper_arm" />
    <link name="lower_arm" />
    <link name="hand" />

    <joint name="joint_base_rotate" type="continuous">
        <parent link="base_link"/>
        <child link="shoulder"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    </joint>
    <joint name="joint_shoulder" type="continuous">
        <parent link="shoulder"/>
        <child link="upper_arm"/>
        <origin xyz = "0 0 0.01" rpy="1.57 0 0" />
        <axix xyz="0 0 1"/>
    </joint>
    <joint name="joint_elbow" type="continuous">
        <parent link="upper_arm"/>
        <child link="lower_arm"/>
        <origin xyz = "0 0 .14605" rpy="0 0 0" />
        <axix xyz="0 1 0"/>
    </joint>
    <joint name="joint_wrist" type="continuous">
        <parent link="lower_arm"/>
        <child link="hand"/>
        <origin xyz = "0 0 .1873" rpy="0 0 0" />
        <axix xyz="1 0 0"/>
    </joint>

    

</robot>
