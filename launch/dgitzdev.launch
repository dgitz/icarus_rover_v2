<!--xml-->
<launch>
 <node name="$(env ROS_HOSTNAME)_timemaster_node" pkg="icarus_rover_v2" type="timemaster_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="0.0"/>
	    <param name="verbosity_level"       value="DEBUG"/> 
        <param name="pps_source"            value="self"/>
        <param name="require_pps_to_start"  value="false"/>
        <param name="loop1_rate"            value="50.0"/> <!-- publish -->
        <param name="loop2_rate"            value="100.0"/> <!-- process update -->
</node>
 <node name="$(env ROS_HOSTNAME)_timeslave_node" pkg="icarus_rover_v2" type="timeslave_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="5.0"/>
	    <param name="verbosity_level"       value="DEBUG"/> 
        <param name="require_pps_to_start"  value="false"/>
        <param name="loop1_rate"            value="10.0"/> <!-- publish -->
</node>
<node name="$(env ROS_HOSTNAME)_topicremapper_node" pkg="icarus_rover_v2" type="topicremapper_node"  output="screen" clear_params="true">
        <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="loop1_rate"            value="20.0"/> <!-- Publish -->
</node>
<node name="$(env ROS_HOSTNAME)_networktransceiver_node" pkg="icarus_rover_v2" type="networktransceiver_node"  output="screen" clear_params="true">
        <param name="verbosity_level"       value="WARN"/>
        <param name="startup_delay"         value="0.0" />
        <param name="require_pps_to_start"  value="false"/> 
        <param name="Send_Multicast_Group"       value="239.255.43.21"/>
        <param name="Send_Multicast_Port"        value="45454"/>
        <param name="Recv_Unicast_Port"          value="5678"/>
        <param name="Mode"                  value="DriverStation"/> <!--Diagnostics_GUI-->
        <param name="loop1_rate"            value="20.0"/> <!-- Send High Priority Messages -->  
        <param name="loop2_rate"            value="5.0"/> <!-- Send Medium and Low Priority Messages, Ready To Arm -->
        <param name="loop3_rate"            value="50.0"/> <!-- Process Update -->
</node>
<node name="$(env ROS_HOSTNAME)_master_node" pkg="icarus_rover_v2" type="master_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="0.0"/>
	    <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/>    
        <param name="loop1_rate"            value="1"/> <!-- device,resource available, -->    
        <param name="loop2_rate"            value="10"/> <!-- process update, -->  
</node>
<node name="$(env ROS_HOSTNAME)_command_node" pkg="icarus_rover_v2" type="command_node"  output="screen" clear_params="true">
        <param name="verbosity_level"       value="DEBUG"/>
        <param name="startup_delay"       value="10.0"/> 
        <param name="require_pps_to_start"  value="false"/> 
        <param name="loop1_rate"            value="20.0"/> <!-- Armed State Publish -->  
        <param name="loop2_rate"            value="50.0"/> <!-- Process Update -->
        <param name="loop3_rate"            value="100.0"/> <!-- Periodic Commands (max) -->
        <param name="ready_to_arm_topic1"   value="/ControlModule1_hatcontroller_node/readytoarm"/>
        <param name="ready_to_arm_topic2"   value="/ControlModule1_safety_node/readytoarm"/>-->
        <param name="ready_to_arm_topic3"   value="/ControlModule2_diagnostic_node/readytoarm"/>
        <param name="ready_to_arm_topic4"   value="/ControlModule2_networktransceiver_node/readytoarm"/>
        <param name="user_command_topic" value="/DriverStation/user_command"/>
</node>
<node name="$(env ROS_HOSTNAME)_datalogger_node" pkg="icarus_rover_v2" type="datalogger_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="1.0"/>
        <param name="verbosity_level"       value="INFO"/>
        <param name="require_pps_to_start"  value="true"/> 
        <param name="LogFile_Duration"      value="1800.0"/>
        <param name="LogFile_Directory"     value="/media/robot/ICARUS/dataLogs/"/>
</node>
<node name="$(env ROS_HOSTNAME)_diagnostic_node" pkg="icarus_rover_v2" type="diagnostic_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="5.0"/>
        <param name="verbosity_level"       value="INFO"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="RAM_usage_threshold_MB"    value="2000"/>
        <param name="CPU_usage_threshold_percent"    value="30"/> 
        <param name="Log_Resources_Used"        value="false"/> 
        <param name="loop1_rate"            value="5.0"/> <!-- Publish System Diagnostics -->   
</node>
 <node name="$(env ROS_HOSTNAME)_imu_node" pkg="icarus_rover_v2" type="imu_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="1.0"/>
	    <param name="verbosity_level"       value="DEBUG"/> 
        <param name="require_pps_to_start"  value="false"/>
        <param name="loop1_rate"            value="50.0"/> <!-- publish -->
</node>
<node name="$(env ROS_HOSTNAME)_audio_node" pkg="icarus_rover_v2" type="audio_node"  output="screen" clear_params="true">
        <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="loop1_rate"            value="5.0"/> 
        <param name="audiostage_directory" value="/home/robot/storage/AUDIO"/>
        <param name="audiofile_length"      value="30"/>
        <param name="audiobuffer_length"    value="30"/>
        <param name="volume_perc"    value="100.0"/>
</node>
<node name="$(env ROS_HOSTNAME)_calibration_node" pkg="icarus_rover_v2" type="calibration_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="0.0"/>
	    <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="loop1_rate"            value="20"/>   
        <param name="loop2_rate"            value="10"/> 
</node>
<node name="$(env ROS_HOSTNAME)_pose_node" pkg="icarus_rover_v2" type="pose_node"  output="screen" clear_params="true">
        <param name="startup_delay"       value="0.0"/>
	    <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="Operation_Mode"        value="CALIBRATE" />
        <param name="loop1_rate"            value="20"/>   
        <param name="loop2_rate"            value="10"/> 
</node>
</launch>
