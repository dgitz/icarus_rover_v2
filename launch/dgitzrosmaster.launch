<!--xml-->
<launch>
	<node name="$(env ROS_HOSTNAME)_commandlauncher_node" pkg="icarus_rover_v2" type="commandlauncher_node" output="screen" clear_params="true">  
	    <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/>
        <param name="loop1_rate"            value="1.0"/> <!-- command launch/check if running -->
        <param name="loop2_rate"            value="50.0"/> <!-- Process Update,Output publish --> 
        <param name="loop3_rate"            value="200.0"/> <!--  -->
		<param name="CameraStream" value="DriverStation:CameraPort"/>
    </node>
    <node name="$(env ROS_HOSTNAME)_imageclassifier_node" pkg="icarus_rover_v2" type="imageclassifier_node.py" output="screen" clear_params="true">
        <param name="image_topic"   value="/ComputeModule1_cameracapture_node/raw_image"/>
    </node>
    
    <node name="$(env ROS_HOSTNAME)_speaker_node" pkg="icarus_rover_v2" type="speaker_node"  output="screen">
        <param name="loop_rate"             value="500"/>
        <param name="verbosity_level"       value="DEBUG"/>
        <param name="require_pps_to_start"  value="false"/> 
        <param name="loop1_rate"            value="50"/>
    </node>
    <node name="$(env ROS_HOSTNAME)_soundplay_node" pkg="sound_play" type="soundplay_node.py">
    </node>
    <!--
    <node name="$(env ROS_HOSTNAME)_targetfinder_node" pkg="icarus_rover_v2" type="targetfinder_node.py" output="screen" clear_params="true">
        <param name="image_topic"   value="/ComputeModule1_cameracapture_node/raw_image"/>
    </node>
    -->
</launch>
